{
  "name": "subtitle-chan",
  "version": "0.0.0",
  "private": true,
  "description": "Live speech transcription and translation in your browser",
  "keywords": [
    "subtitle",
    "transcription",
    "translation",
    "speech",
    "google",
    "translate"
  ],
  "homepage": "https://ae9is.github.io/subtitle-chan",
  "bugs": {
    "url": "https://github.com/ae9is/subtitle-chan/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ae9is/subtitle-chan.git"
  },
  "author": {
    "name": "ae9is",
    "email": "aegis.opensource@gmail.com"
  },
  "license": "MIT",
  "files": [
    "docs"
  ],
  "sideEffects": [
    "*.css",
    "*.png"
  ],
  "scripts": {
    "build": "run-s build:*",
    "build:clean": "rm -rf .tsbuildinfo docs",
    "build:vite": "vite build",
    "dev": "vite --port 3000",
    "test": "npm run vitest",
    "vitest": "vitest run",
    "format": "prettier --write src",
    "lint": "eslint src",
    "preview": "vite preview --host 0.0.0.0 --outDir docs",
    "update": "npm run update-packages && npm install && npm run build && npm run test",
    "update-packages": "ncu -u"
  },
  "dependencies": {
    "@vitejs/plugin-react-swc": "^3.3.1",
    "axios": "^1.4.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-speech-recognition": "^3.10.0",
    "regenerator-runtime": "^0.13.11",
    "vite": "^4.3.9"
  },
  "devDependencies": {
    "@testing-library/dom": "^9.3.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/node": "^20.2.5",
    "@types/react": "^18.2.8",
    "@types/react-dom": "^18.2.4",
    "@types/react-speech-recognition": "^3.9.2",
    "jsdom": "^22.1.0",
    "npm-check-updates": "^16.10.12",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.8",
    "typescript": "^5.1.3",
    "vitest": "^0.31.4",
    "eslint": "^8.41.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "@typescript-eslint/eslint-plugin": "^5.59.8",
    "@typescript-eslint/parser": "^5.59.8"
  }
}
